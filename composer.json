{
    "name": "magentron/ciphersweet-for-laravel",
    "description": "CipherSweet for Laravel (forked from bjornvoesten/ciphersweet-for-laravel)",
    "license": "MIT",
    "authors": [
        {
            "name": "Bjorn Voesten",
            "email": "bjorn-voesten@live.nl"
        },
        {
            "name": "Lopoi"
        }
    ],
    "autoload": {
        "psr-4": {
            "BjornVoesten\\CipherSweet\\": "src/",
            "BjornVoesten\\CipherSweet\\Seeds\\": "database/seeds/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replaces": {
		"bjornvoesten/ciphersweet-for-laravel": "1.4.0"
	},
    "require": {
        "php": "^7.2.5|^8.0|^8.1|^8.2",
        "illuminate/config": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0|^11.0",
        "paragonie/ciphersweet": "^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BjornVoesten\\CipherSweet\\CipherSweetServiceProvider"
            ]
        }
    }
}
